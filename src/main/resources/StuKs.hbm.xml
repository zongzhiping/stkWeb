<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.StuKs" table="stuks">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" length="50" />
        </property>
        
        <property name="userNo" type="java.lang.String">
            <column name="userNo" length="50" />
        </property>
        <property name="realName" type="java.lang.String">
            <column name="realName" length="50" />
        </property>
        <property name="sex" type="java.lang.String">
            <column name="sex" length="50" />
        </property>
        <property name="xy" type="java.lang.String">
            <column name="xy" length="50" />
        </property>
        <property name="zy" type="java.lang.String">
            <column name="zy" length="50" />
        </property>
        <property name="bj" type="java.lang.String">
            <column name="bj" length="50" />
        </property>
        <property name="kid" type="java.lang.Integer">
            <column name="kid"  />
        </property>
        <property name="fs" type="java.lang.Integer">
            <column name="fs"  />
        </property>
         <property name="stuid" type="java.lang.Integer">
            <column name="stuid" />
        </property>
        
         <property name="state" type="java.lang.Integer">
            <column name="state" />
        </property>
    </class>
</hibernate-mapping>
