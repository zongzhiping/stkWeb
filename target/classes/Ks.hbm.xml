<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.Ks" table="ks">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
          <property name="title" type="java.lang.String">
            <column name="title" length="50" />
        </property>
        <property name="times" type="java.lang.String">
            <column name="times" length="50" />
        </property>
         <property name="state" type="java.lang.Integer">
            <column name="state" />
        </property>
         <property name="dx" type="java.lang.Integer">
            <column name="dx"  />
        </property>
        <property name="ddx" type="java.lang.Integer">
            <column name="ddx"  />
        </property>
        <property name="pd" type="java.lang.Integer">
            <column name="pd"  />
        </property>
        <property name="bc" type="java.lang.Integer">
            <column name="bc"  />
        </property>
        
         <property name="type" type="java.lang.Integer">
            <column name="type"  />
        </property>
        
        
    </class>
</hibernate-mapping>
