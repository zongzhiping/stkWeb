<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.Tm" table="tm">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
          <property name="type" type="java.lang.Integer">
            <column name="type" />
        </property>
        <property name="tid" type="java.lang.Integer">
            <column name="tid" />
        </property>
        <property name="kid" type="java.lang.Integer">
            <column name="kid" />
        </property>
         <property name="fs" type="java.lang.Integer">
            <column name="fs" />
        </property>
         <property name="stuid" type="java.lang.Integer">
            <column name="stuid" />
        </property>
         <property name="stufs" type="java.lang.Integer">
            <column name="stufs" />
        </property>
         <property name="title" type="java.lang.String">
            <column name="title" length="50" />
        </property>
        <property name="a" type="java.lang.String">
            <column name="a" length="50" />
        </property>
         <property name="b" type="java.lang.String">
            <column name="b" length="50" />
        </property>
         <property name="c" type="java.lang.String">
            <column name="c" length="50" />
        </property>
         <property name="d" type="java.lang.String">
            <column name="d" length="50" />
        </property>
        
        <property name="da" type="java.lang.String">
            <column name="da" length="50" />
        </property>
          <property name="studa" type="java.lang.String">
            <column name="studa" length="50" />
        </property>
    </class>
</hibernate-mapping>
